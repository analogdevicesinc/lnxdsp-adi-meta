From 06487f1324b93b26087ac50604c5e3c5203ca201 Mon Sep 17 00:00:00 2001
From: Caleb Ethridge <caleb.ethridge@analog.com>
Date: Fri, 5 Sep 2025 09:27:00 -0400
Subject: [PATCH] Enabled SD card support for SC598 SOM

Signed-off-by: Caleb Ethridge <caleb.ethridge@analog.com>
---
 arch/arm/dts/sc598-som.dtsi | 9 +++++----
 1 file changed, 5 insertions(+), 4 deletions(-)

diff --git a/arch/arm/dts/sc598-som.dtsi b/arch/arm/dts/sc598-som.dtsi
index 02b46e5fff..3924f925c2 100644
--- a/arch/arm/dts/sc598-som.dtsi
+++ b/arch/arm/dts/sc598-som.dtsi
@@ -54,8 +54,9 @@
 			pinctrl-0 = <&mmc_defaults>;
 			clocks = <&emmcclk>;
 			clock-names = "core";
-			max-frequency = <50000000>;
-			bus-width = <8>;
+			max-frequency = <44000000>;
+			no-1-8-v;
+			bus-width = <4>;
 			u-boot,dm-pre-reloc;
 		};
 	};
@@ -197,7 +198,7 @@
 		emmc {
 			gpio-hog;
 			gpios = <8 GPIO_ACTIVE_LOW>;
-			output-high;
+			output-low;
 			line-name = "emmc-en";
 			u-boot,dm-pre-reloc;
 		};
@@ -205,7 +206,7 @@
 		emmc-som-en {
 			gpio-hog;
 			gpios = <9 GPIO_ACTIVE_LOW>;
-			output-low;
+			output-high;
 			line-name = "emmc-som-en";
 			u-boot,dm-pre-reloc;
 		};
-- 

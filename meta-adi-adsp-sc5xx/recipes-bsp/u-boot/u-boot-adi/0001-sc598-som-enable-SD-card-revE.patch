From 45ac2b0c650c68cf2a86a85885f38df13779b9c2 Mon Sep 17 00:00:00 2001
From: Caleb Ethridge <caleb.ethridge@analog.com>
Date: Mon, 8 Sep 2025 11:07:49 -0400
Subject: [PATCH] Enable SD card support for SC598 SOM Rev E

---
 arch/arm/dts/sc598-som.dtsi | 9 +++++----
 1 file changed, 5 insertions(+), 4 deletions(-)

diff --git a/arch/arm/dts/sc598-som.dtsi b/arch/arm/dts/sc598-som.dtsi
index e526eb1b6a..0edd26b781 100644
--- a/arch/arm/dts/sc598-som.dtsi
+++ b/arch/arm/dts/sc598-som.dtsi
@@ -54,8 +54,9 @@
 			pinctrl-0 = <&mmc_defaults>;
 			clocks = <&emmcclk>;
 			clock-names = "core";
-			max-frequency = <50000000>;
-			bus-width = <8>;
+			max-frequency = <44000000>;
+			no-1-8-v;
+			bus-width = <4>;
 			u-boot,dm-pre-reloc;
 		};
 	};
@@ -173,7 +174,7 @@
 		emmc {
 			gpio-hog;
 			gpios = <8 GPIO_ACTIVE_LOW>;
-			output-high;
+			output-low;
 			line-name = "emmc-en";
 			u-boot,dm-pre-reloc;
 		};
@@ -181,7 +182,7 @@
 		emmc-som-en {
 			gpio-hog;
 			gpios = <9 GPIO_ACTIVE_LOW>;
-			output-low;
+			output-high;
 			line-name = "emmc-som-en";
 			u-boot,dm-pre-reloc;
 		};
-- 
2.34.1

